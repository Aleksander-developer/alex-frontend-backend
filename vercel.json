{
  "version": 2,
  "builds": [
    {
      "src": "frontend/angular.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist/AlexFEDeveloper"
      }
    },
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/AlexFEDeveloper/$1"
    }
  ]
}
